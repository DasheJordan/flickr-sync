{
    "config": {
        "autoloader-suffix": "IJENKO",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Itscaro\\": "src/"
        }
    },
    "bin": ["bin/run.php"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/itscaro/phpflickr"
        },
        {
            "type": "vcs",
            "url": "https://github.com/itscaro/zf2-restful"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zendframework/zendoauth"
        }
    ],
    "require": {
        "Symfony/Console": "*",
        "Symfony/Finder": "*",
        "Symfony/Filesystem": "*",
        "Symfony/Process": "*",
        "Symfony/Yaml": "*",
        "Symfony/Config": "*",
        "itscaro/zf2-restful": "dev-master",
        "itscaro/phpflickr": "dev-master",
        "dopiaza/dpzflickr": "dev-master"
    },
    "require-dev": {
        "kherge/box": "*"
    }
}
